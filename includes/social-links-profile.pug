section.social
  figure
    img.social__avatar(
      src='./assets/images/avatar-jessica.jpeg',
      alt='Avatar Jessica'
    )

  header.social__header
    h1.social__header--name Jessica Randall
    h2.social__header--location London, United Kingdom
    p.social__header--quote
      q Front-end developer and avid reader.

  nav.social__links
    ul.social__links--list
      li.social__links--list-item
        a.social__links--list-item--link(href='#') GitHub
      li.social__links--list-item
        a.social__links--list-item--link(href='#') Frontend Mentor
      li.social__links--list-item
        a.social__links--list-item--link(href='#') LinkedIn
      li.social__links--list-item
        a.social__links--list-item--link(href='#') Twitter
      li.social__links--list-item
        a.social__links--list-item--link(href='#') Instagram
